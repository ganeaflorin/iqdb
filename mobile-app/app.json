{
  "expo": {
    "name": "IQDB",
    "slug": "IQDB",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#157173"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#157173"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ganeaflorin.iqdb"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Aplicația are nevoie de acces la galerie pentru a putea încărca o imagine."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1d470655-a5b1-4cc3-acbc-cfd1716d2fda"
      }
    }
  }
}